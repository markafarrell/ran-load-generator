[Unit]
Description=sessionControllerServer uWSGI app
After=syslog.target

[Service]
ExecStart=/usr/bin/uwsgi --ini /etc/ran-load-generator/sessionControllerServer.ini
RuntimeDirectory=sessionControllerServer
RuntimeDirectoryMode=0777
Restart=on-failure
KillSignal=SIGQUIT
Type=notify
StandardError=syslog
NotifyAccess=all

[Install]
WantedBy=multi-user.target
